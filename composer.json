{
    "name": "oskarstark/env-var-extension",
    "type": "library",
    "description": "A Twig extension to access env variables",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "twig/twig": "^2.6 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "ergebnis/test-util": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OskarStark\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OskarStark\\Tests\\Twig\\": "tests/"
        }
    }
}
